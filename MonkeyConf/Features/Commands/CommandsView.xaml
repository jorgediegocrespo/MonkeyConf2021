<?xml version="1.0" encoding="utf-8" ?>
<base:BaseContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                      xmlns:base="clr-namespace:MonkeyConf.Base"                      
                      xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                      xmlns:features="clr-namespace:MonkeyConf.Features.Commands"
                      x:TypeArguments="features:CommandsViewModel"
                      x:Class="MonkeyConf.Features.Commands.CommandsView">

    <base:BaseContentPage.Content>
        <StackLayout VerticalOptions="Center" HorizontalOptions="Center">
            <Button x:Name="btSlow" Text="Slow" />
            <Button x:Name="btQuick" Text="Quick" />
            <ActivityIndicator x:Name="aiBusy" IsVisible="True" IsRunning="True"/>
        </StackLayout>
    </base:BaseContentPage.Content>

</base:BaseContentPage>